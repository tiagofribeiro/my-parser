  "type": "module",
  "scripts": {
    "client:dev": "webpack serve --mode development --open",
    "client:build": "webpack --mode production",
    "server:dev": "nodemon --exec ts-node src/server/index.ts",
    "server:build": "tsc",
    "server:prod": "node dist/index.js",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "npm run build && npm run server:prod"
  },
